	<td width="99" class="biaoti1">
		<div class="rl_nav"><span>第 <script language="JavaScript">
		Date.prototype.getWeek = function(startDate) {
		var onejan = new Date(startDate);
		return Math.ceil((((this - onejan) / 86400000) + onejan.getDay()-1)/7);
		}
		var scoolBeginDate = "2018/09/03"   <!--学期开始时间设置-->
		var now = new Date();
		var weekno = now.getWeek(scoolBeginDate);
