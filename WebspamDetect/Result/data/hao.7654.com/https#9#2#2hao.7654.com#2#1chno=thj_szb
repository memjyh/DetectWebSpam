                document.write('<link rel="stylesheet" href="http://cdn2.guangsuss.com/tpl/css/page_lg3688.css?=1542511914">');
            }else{
                document.write('<link rel="stylesheet" href="http://cdn3.guangsuss.com/tpl/css/page_lg.css?=1542511914">'); 
            }
            document.write('<link rel="stylesheet" href="http://cdn4.guangsuss.com/tpl/css/news_lg.css?=1542511914">');
            
        } else {
            if(_browser>-1){
                document.write('<link rel="stylesheet" href="http://cdn1.guangsuss.com/tpl/css/page_new3688.css?=1542511914">');
            }else{
