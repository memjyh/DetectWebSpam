<script>
    $("#banner_close").click(function () {
        $("#shop_banner div ,#shop_banner  ").slideToggle(200, function () {
            if ($("#shop_banner").css('display') == 'none') {
                $("#banner_close").css('background', "url(/tpl/img/2017/618/banner_open.gif) no-repeat center center");
                $("#banner_close").attr('data-tjname', 'jd618,layer_top,btn,open,');
            } else {
                $("#banner_close").css('background', "url(/tpl/img/2017/618/banner_close.png) no-repeat center center");
                $("#banner_close").attr('data-tjname', 'jd618,layer_top,btn,close,');
            }
        });
    })
    $('.shop_banner333,#banner_close,.gg_top_long').on('click', function () {
        var $this = $(this),
                tjname = $this.attr('data-tjname'),
                tjnameArr = tjname.split(',');
        if (!tjname) {
            return;
        }
        var scope = dhLog;
        scope.sendActiveLog({
            firstCtg: tjnameArr[0] || 'null',
            secondCtg: tjnameArr[1] || 'null',
            thirdCtg: tjnameArr[2] || 'null',
            linkName: tjnameArr[3] || $this.attr('data-name'),
            linkIdx: 0,
            linkUrl: $this.attr('href') || $this.attr('data-url')
        });
    });

</script>



