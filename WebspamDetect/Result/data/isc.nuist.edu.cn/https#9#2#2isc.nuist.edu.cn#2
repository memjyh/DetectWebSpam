<script language="JavaScript">
            var w6imgUrl = new Array();
            var w6imgLink = new Array();
            var w6imgTitle = new Array();

            var w6count = 0;
            var w6running = false;
            var w6index = 0;
            var w6nextAdTimer;
            var w6k = 0;
            function w6nextAd(num) {
                if (num >= 0 && num <= w6count - 1) {
                    w6index = num;
                    clearTimeout(w6nextAdTimer);
                } else {
                    num = w6index;
                }
                if (w6index > w6count - 1) {
                    w6index = 0;
                }
                if (!w6running) {
                    w6running = true;

                } else if (document.all) {
                    w6imgShow.filters.revealTrans.Transition = 50;
                    w6imgShow.filters.revealTrans.apply();
                    w6imgShow.filters.revealTrans.play();
                }
                document.getElementById('w6imgShow').src = w6imgUrl[w6index];
                if (w6imgTitle[w6index] != '') {
                    document.getElementById('w6imgTitle').innerHTML = '<a href="javascript:w6openWindow();" title="' + w6imgTitle[w6index] + '">' + w6imgTitle[w6index] + '</a>';
                }
                document.getElementById('w6imgNumber').innerHTML = "";
                w6k = 0;
                for (var i = 0; i < w6count; i++) {
                    w6k++;
                    if (i != w6index) {
                        document.getElementById('w6imgNumber').innerHTML += '<a href="javascript:w6nextAd(' + i + ')" class="nomal">' + w6k + '</a>';
                    } else {
                        document.getElementById('w6imgNumber').innerHTML += '<a href="javascript:w6nextAd(' + i + ')" class="current">' + w6k + '</a>';
                    }
                }

                w6index++;
                if (w6count > 1) {
                    w6nextAdTimer = setTimeout("w6nextAd(-1)", 4000);
                }
            }

            function w6openWindow() {
                var jumpUrl = w6imgLink[w6index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

            $().ready(function() {
                try {
                    var objs = w6imgJsons;
                    w6count = objs.length;
                    if (w6count == 0) {
                        document.getElementById('w6imgShow').style.display = "none";
                        document.getElementById('w6imgNumber').style.display = "none";
                        document.getElementById('w6imgTitle').style.display = "none";
                        return;
                    }
                    for (var i = 0; i < objs.length; i++) {
                        w6imgUrl[i] = objs[i].src;
                        w6imgLink[i] = objs[i].url;
                        w6imgTitle[i] = objs[i].title;
                    }
                    w6nextAd(-1);
                } catch (e) {

                }
            });
