$(function(){
	$("#menu .menu_one").hover(function(){
		$("a.item_name", $(this)).addClass("hover");
		$(".menu_expand", $(this)).slideDown(300);
	}, function(){
		$current = $(this);
		$(".menu_expand", $(this)).hide();
		$("a.item_name", $(this)).removeClass("hover");
	});
});
</script>
