                <td class=" articlelist2_td_title "><a class=" articlelist2_a_title " target="_blank" href="/98/8c/c576a170124/page.htm" title="2016年7-8月份工作简报">2016年7-8月份工作简报</a></td><td class=" articlelist2_td_issuetime "><div style="white-space:nowrap">2016-11-29</div></td>
            </tr>
        </table>
    </div>
    <div id="bottomw45"></div>
</div>

<script language="javascript">
    var middlew45 = document.getElementById("middlew45");
    var bottomw45 = document.getElementById("bottomw45");
    middlew45.innerHTML = middlew45.innerHTML;
    middlew45.innerHTML = middlew45.innerHTML + middlew45.innerHTML + middlew45.innerHTML;
    bottomw45.innerHTML = middlew45.innerHTML + middlew45.innerHTML + middlew45.innerHTML;

    function marqueecw45() {
        if (document.getElementById('bottomw45').offsetTop - document.getElementById('topw45').scrollTop <= 0) {
            document.getElementById('topw45').scrollTop -= document.getElementById('middlew45').offsetHeight;
        } else {
            document.getElementById('topw45').scrollTop += 1;
        }
    }

    function startscrollcw45() {
        var scrollobjc = setInterval(marqueecw45, 150);
        document.getElementById('topw45').onmouseover = function() {
            clearInterval(scrollobjc);
        };
        document.getElementById('topw45').onmouseout = function() {
            scrollobjc = setInterval(marqueecw45, 150);
        };
    }
    startscrollcw45();
</script>
