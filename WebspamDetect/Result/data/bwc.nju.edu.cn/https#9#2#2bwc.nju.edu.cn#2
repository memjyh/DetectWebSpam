<div class="dtjt8_div_img" style="width:240px;height:180px;">
    <img id="w46imgShow" name="w46imgShow" onclick="w46openWindow();" style="FILTER: revealTrans(duration=2,transition=20); border: 0; cursor: pointer;" src="/..//_css/tpl/default/images/loading.gif" width="240px" height="180px" vspace="0">
    <div id="w46imgNumber" class="number"></div>
</div>
<script language="JavaScript" class="">
    var w46imgJsons = [
    
        
            {
            "src" : "/_upload/article/images/09/ad/2a79b20742819eca7e9b9197f8f1/86bf4dd8-250b-4821-99d5-20e78e36db7c_s.jpg",
            "url" : "/6c/30/c576a224304/page.htm"
            },
        
    
        
            {
            "src" : "/_upload/article/75/a7/c56b024e4498a107db4667a5aabb/e0d84621-7ecf-4fc6-8729-38f16e09a061_s.jpg",
            "url" : "/4f/6b/c576a20331/page.htm"
            }];
        
       
</script>
<script language="JavaScript">
        var w46imgUrl = new Array();
        var w46imgLink = new Array();

        var w46count = 0;
        var w46running = false;
        var w46index = 0;
        var w46nextAdTimer;
        var w46k = 0;
        function w46nextAd(num) {
            if (num >= 0 && num <= w46count - 1) {
                w46index = num;
                clearTimeout(w46nextAdTimer);
            } else {
                num = w46index;
            }
            if (w46index > w46count - 1) {
                w46index = 0;
            }
            if (!w46running) {
                w46running = true;

            } else if (document.all) {
                w46imgShow.filters.revealTrans.Transition = 50;
                w46imgShow.filters.revealTrans.apply();
                w46imgShow.filters.revealTrans.play();
            }
            document.getElementById('w46imgShow').src = w46imgUrl[w46index];
            document.getElementById('w46imgNumber').innerHTML = "";
            w46k = 0;
            for (var i = 0; i < w46count; i++) {
                w46k++;
                if (i !== w46index) {
                    document.getElementById('w46imgNumber').innerHTML += '<a href="javascript:w46nextAd(' + i + ')" class="nomal">' + w46k + '</a>';
                } else {
                    document.getElementById('w46imgNumber').innerHTML += '<a href="javascript:w46nextAd(' + i + ')" class="current">' + w46k + '</a>';
                }
            }
            w46index++;
            if (w46count > 1) {
                w46nextAdTimer = setTimeout("w46nextAd(-1)", 4000);
            }
        }

        function w46openWindow() {
            var jumpUrl = w46imgLink[w46index - 1];
            if (jumpUrl && jumpUrl != '') {
                window.open(jumpUrl);
            }
        }

        $().ready(function() {
            try {
                var objs = w46imgJsons;
                w46count = objs.length;
                if (w46count == 0) {
                    document.getElementById('w46imgShow').style.display = "none";
                    document.getElementById('w46imgNumber').style.display = "none";
                    return;
                }
                for (var i = 0; i < objs.length; i++) {
                    w46imgUrl[i] = objs[i].src;
                    w46imgLink[i] = objs[i].url;
                }
                w46nextAd(-1);
            } catch (e) {
            }
        });
</script>
