<script language="JavaScript">
        var w66imgUrl = new Array();
        var w66imgLink = new Array();

        var w66count = 0;
        var w66running = false;
        var w66index = 0;
        var w66nextAdTimer;
        var w66k = 0;
        function w66nextAd(num) {
            if (num >= 0 && num <= w66count - 1) {
                w66index = num;
                clearTimeout(w66nextAdTimer);
            } else {
                num = w66index;
            }
            if (w66index > w66count - 1) {
                w66index = 0;
            }
            if (!w66running) {
                w66running = true;

            } else if (document.all) {
                w66imgShow.filters.revealTrans.Transition = 50;
                w66imgShow.filters.revealTrans.apply();
                w66imgShow.filters.revealTrans.play();
            }
            document.getElementById('w66imgShow').src = w66imgUrl[w66index];
            document.getElementById('w66imgNumber').innerHTML = "";
            w66k = 0;
            for (var i = 0; i < w66count; i++) {
                w66k++;
                if (i !== w66index) {
                    document.getElementById('w66imgNumber').innerHTML += '<a href="javascript:w66nextAd(' + i + ')" class="nomal">' + w66k + '</a>';
                } else {
                    document.getElementById('w66imgNumber').innerHTML += '<a href="javascript:w66nextAd(' + i + ')" class="current">' + w66k + '</a>';
                }
            }
            w66index++;
            if (w66count > 1) {
                w66nextAdTimer = setTimeout("w66nextAd(-1)", 4000);
            }
        }

        function w66openWindow() {
            var jumpUrl = w66imgLink[w66index - 1];
            if (jumpUrl && jumpUrl != '') {
                window.open(jumpUrl);
            }
        }

        $().ready(function() {
            try {
                var objs = w66imgJsons;
                w66count = objs.length;
                if (w66count == 0) {
                    document.getElementById('w66imgShow').style.display = "none";
                    document.getElementById('w66imgNumber').style.display = "none";
                    return;
                }
                for (var i = 0; i < objs.length; i++) {
                    w66imgUrl[i] = objs[i].src;
                    w66imgLink[i] = objs[i].url;
                }
                w66nextAd(-1);
            } catch (e) {
            }
        });
</script>
            
