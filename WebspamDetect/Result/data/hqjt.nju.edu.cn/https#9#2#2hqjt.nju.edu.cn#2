                    }
                }

                w10index++;
                if (w10count > 1) {
                    w10nextAdTimer = setTimeout("w10nextAd(-1)", 4000);
                }
            }

            function w10openWindow() {
                var jumpUrl = w10imgLink[w10index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

