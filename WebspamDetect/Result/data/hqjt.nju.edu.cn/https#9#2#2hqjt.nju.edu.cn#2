            function w10nextAd(num) {
                if (num >= 0 && num <= w10count - 1) {
                    w10index = num;
                    clearTimeout(w10nextAdTimer);
                } else {
                    num = w10index;
                }
                if (w10index > w10count - 1) {
                    w10index = 0;
                }
                if (!w10running) {
                    w10running = true;

