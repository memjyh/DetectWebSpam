<script language="JavaScript">
            var w31imgUrl = new Array();
            var w31imgLink = new Array();
            var w31imgTitle = new Array();

            var w31count = 0;
            var w31running = false;
            var w31index = 0;
            var w31nextAdTimer;
            var w31k = 0;
            function w31nextAd(num) {
                if (num >= 0 && num <= w31count - 1) {
                    w31index = num;
                    clearTimeout(w31nextAdTimer);
                } else {
                    num = w31index;
                }
                if (w31index > w31count - 1) {
                    w31index = 0;
                }
                if (!w31running) {
                    w31running = true;

                } else if (document.all) {
                    w31imgShow.filters.revealTrans.Transition = 50;
                    w31imgShow.filters.revealTrans.apply();
                    w31imgShow.filters.revealTrans.play();
                }
                document.getElementById('w31imgShow').src = w31imgUrl[w31index];
                if (w31imgTitle[w31index] != '') {
                    document.getElementById('w31imgTitle').innerHTML = '<a href="javascript:w31openWindow();" title="' + w31imgTitle[w31index] + '">' + w31imgTitle[w31index] + '</a>';
                }
                document.getElementById('w31imgNumber').innerHTML = "";
                w31k = 0;
                for (var i = 0; i < w31count; i++) {
                    w31k++;
                    if (i != w31index) {
                        document.getElementById('w31imgNumber').innerHTML += '<a href="javascript:w31nextAd(' + i + ')" class="nomal">' + w31k + '</a>';
                    } else {
                        document.getElementById('w31imgNumber').innerHTML += '<a href="javascript:w31nextAd(' + i + ')" class="current">' + w31k + '</a>';
                    }
                }

                w31index++;
                if (w31count > 1) {
                    w31nextAdTimer = setTimeout("w31nextAd(-1)", 4000);
                }
            }

            function w31openWindow() {
                var jumpUrl = w31imgLink[w31index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

            $().ready(function() {
                try {
                    var objs = w31imgJsons;
                    w31count = objs.length;
                    if (w31count == 0) {
                        document.getElementById('w31imgShow').style.display = "none";
                        document.getElementById('w31imgNumber').style.display = "none";
                        document.getElementById('w31imgTitle').style.display = "none";
                        return;
                    }
                    for (var i = 0; i < objs.length; i++) {
                        w31imgUrl[i] = objs[i].src;
                        w31imgLink[i] = objs[i].url;
                        w31imgTitle[i] = objs[i].title;
                    }
                    w31nextAd(-1);
                } catch (e) {

                }
            });
</script>

