        <script type="text/javascript">
            $(document).ready(function(){
                $(".footlink").change(function(){
                   $(this).parent().find(".golink").attr("href", $("#"+$(this).attr("id")+" option:selected").val());
                })
                //$(".golink").click(function(){window.open($(this).parent().find(".footlink option:selected").val());});
                $("#favoriteSite").click(function(){
                    addfavorite('http://elite.nju.edu.cn','南京大学基础学科拔尖计划');
                });
            })
            function addfavorite(url,name){
                if (document.all){window.external.addFavorite(url,name);}
                else if (window.sidebar){window.sidebar.addPanel(name, url, "");}
            } 
            function VerifyLogin(){
                var la=$("#txtAccount").val().length;
                var lp=$("#txtPw").val().length;
                if(la<=0||lp<=0){alert("用户名和密码不能为空！");return false;}
                else{$("#loginalert").hide();return true;}
            }
            function VerifySearch(){
                var ls=$("#txtSearch").val().length;
                if(ls<=0){alert("请输入需要检索的关键字！");return false;}else{return true;}
            }
        </script>

